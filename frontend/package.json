{
  "name": "sick-fits-frontend",
  "version": "1.0.0",
  "author": "Wes",
  "license": "ISC",
  "description": "Sick Fits! An example React, GraphQL, Next and Apollo",
  "main": "index.js",
  "scripts": {
    "dev": "next dev -p 7777",
    "build": "next build",
    "start": "next start -p 7777",
    "lint": "eslint --fix \"{,!(node_modules|.next)/**/}*.{js,jsx,ts,tsx}\"",
    "format": "prettier -w -u \"{,!(node_modules|.next)/**/}*.{js,jsx,ts,tsx}\"",
    "schemagenerate": "apollo schema:download --endpoint http://localhost:4000/api/graphql ./lib/graphql/generated/schema.graphqls",
    "codegen": "apollo client:codegen  --target typescript --localSchemaFile ./lib/graphql/generated/schema.graphqls --outputFlat --includes \"lib/graphql/**/*.ts\" --excludes \"lib/graphql/generated/**\" --addTypename --globalTypesFile \"lib/graphql/generated/graphql-global-types.ts\" ./lib/graphql/generated",
    "test": "jest"
  },
  "dependencies": {
    "@apollo/client": "^3.7.7",
    "@atlaskit/button": "^16.5.3",
    "@atlaskit/checkbox": "^12.4.4",
